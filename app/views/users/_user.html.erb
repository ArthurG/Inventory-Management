<tr>
	<td><%= user.email %></td>
	<td><%= user.user_type %></td>
	<% if current_user.user_type == "Owner" || 
		current_user.user_type == "HR Manager" %>
		<td><%= link_to "Edit", edit_user_path(user), class: "btn btn-default" %></td>
		<td><%= link_to "Delete", user, method: :delete, class: "btn btn-default" %></td>
	<% end %>
</tr>

